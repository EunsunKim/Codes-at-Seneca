/*  sourse from https://www.hackerrank.com/challenges/compare-the-triplets  */
#include <iostream>

using namespace std;

int main(){
	int A[3], B[3];
	for (int i = 0; i < 3; i++)
	{
		cin >> A[i];
	}
	for (int i = 0; i < 3; i++)
	{
		cin >> B[i];
	}
	int Apoint = 0, Bpoint=0;
	for (int i = 0; i < 3; i++)
	{
		if (A[i] > B[i]) Apoint++;
		if (A[i] < B[i]) Bpoint++;
	}
	cout << Apoint << " " << Bpoint;
    return 0;
}
